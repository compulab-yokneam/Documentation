autoload=no
baseboard_i2c_enable=true
baudrate=115200
board_name=CL-SOM-iMX7
bootcmd=echo SD boot attempt ...; run sdbootscript; run sdboot; echo eMMC boot attempt ...; run emmcbootscript; run emmcboot; echo USB boot attempt ...; run usbbootscript; echo NAND boot attempt  ...; run nandboot
bootdelay=3
bootscript=echo Running bootscript from ${storagetype} ...; source ${loadaddr};
console=ttymxc0
displaytype=dvi
doboot=bootz ${loadaddr} - ${fdtaddr}
emmcboot=setenv mmcdev 1; setenv mmcblk 2; run mmcboot
emmcbootscript=setenv mmcdev 1; setenv mmcblk 2; run mmcbootscript
eth1addr=00:01:c0:2d:a1:32
ethact=FEC0
ethaddr=00:01:c0:2d:a1:31
ethprime=FEC
fdt_high=0xffffffff
fdtaddr=0x83000000
fdtfile=imx7d-sbc-imx7.dtb
initrd_high=0xffffffff
kernel=zImage
kernel_size=800000
loadaddr=0x80800000
loadfdt=load ${storagetype} ${storagedev} ${fdtaddr} ${fdtfile};
loadfdt_nand=nand read ${fdtaddr} 980000 10000
loadkernel=load ${storagetype} ${storagedev} ${loadaddr} ${kernel};
loadkernel_nand=nand read ${loadaddr} 0 ${kernel_size}
loadscript=load ${storagetype} ${storagedev} ${loadaddr} ${script};
mmc_config=mmc dev ${mmcdev}; mmc rescan
mmcargs=setenv bootargs console=${console},${baudrate} root=/dev/mmcblk${mmcblk}p2 rootwait rw
mmcboot=if run mmc_config; then setenv storagetype mmc;setenv storagedev ${mmcdev}:${mmcpart};if run loadkernel; then if run loadfdt; then run storagebootcmd;fi; fi; fi;
mmcbootscript=if run mmc_config; then setenv storagetype mmc;setenv storagedev ${mmcdev}:${mmcpart};if run loadscript; then run bootscript; fi; fi;
mmcdev_def=0
mmcpart=1
nandargs=setenv bootargs console=${console},${baudrate} root=ubi0:rootfs rw rootfstype=ubifs ubi.mtd=rootfs
nandboot=setenv storagetype nand;if run loadkernel_nand; then if run loadfdt_nand; then run storagebootcmd;fi; fi;
preboot=usb start
script=boot.scr
sdboot=setenv mmcdev ${mmcdev_def}; setenv mmcblk 0; run mmcboot
sdbootscript=setenv mmcdev ${mmcdev_def}; setenv mmcblk 0; run mmcbootscript
serial#=5A1996200
serialpn=C1000D-D2G-N32G-E2-A-I-UH-WAB-V140-230424-0001c02da131
soc=imx7d
stderr=serial,vga
stdin=serial,usbkbd
stdout=serial,vga
storagebootcmd=echo Booting from ${storagetype} ...; run ${storagetype}args; run doboot;
usbbootscript=setenv usbdev ${usbdev_def}; setenv storagetype usb;setenv storagedev ${usbdev}:${usbpart};if run loadscript; then run bootscript; fi;
usbdev_def=0
usbpart=1
videomode=video=ctfb:x:1024,y:768,depth:24,pclk:15384,le:168,ri:8,up:29,lo:3,hs:144,vs:4,sync:0,vmode:0
